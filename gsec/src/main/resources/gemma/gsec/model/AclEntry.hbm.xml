<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="gemma.gsec.acl.domain.AclEntry" table="ACLENTRY">
      <cache usage="read-write"/>
      <id name="id" type="java.lang.Long" unsaved-value="null" access="field">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <property name="granting" type="java.lang.Boolean" access="field">
         <column name="GRANTING" not-null="true" unique="false" sql-type="TINYINT"/>
      </property>
      <property name="mask" type="java.lang.Integer" access="field">
         <column name="MASK" not-null="true" unique="false" sql-type="INTEGER"/>
      </property>
      <property name="aceOrder" type="java.lang.Integer" access="field">
         <column name="ACE_ORDER" not-null="true" unique="false" sql-type="INTEGER"/>
      </property>
      <many-to-one name="sid" class="gemma.gsec.acl.domain.AclSid" lazy="false" fetch="join" access="field">
         <column name="SID_FK" not-null="true" sql-type="BIGINT"/>
      </many-to-one>
   </class>
</hibernate-mapping>
